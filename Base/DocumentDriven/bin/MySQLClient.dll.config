<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!-- 用几个数据库及实例化方式 伯乐灯 会员-->
    <add key="MetadataKind" value="2"/>
    <!-- 客户数据库类型-->
    <add key="DataBaseType" value="57"/>
    <!-- 项目标识，区分session-->
    <add key="UserLoginSign" value="MySQLMange"/>
    <!-- cookies 加密的密钥-->
    <add key="UserLoginKey" value="34446787"/>
    <!-- 分页控件使用的一个密钥-->
    <add key="PagerSQLKey" value="85496032"/>
    <!-- sso端的访问网址，不加斜杠-->
    <add key="SSOURL" value=""/>
    <!-- 设置统一分配的网站应用ID 1:-->
    <add key="WebappID" value="1"/>
    <!-- 网站应用端加密解密app端票据的密钥-->
    <add key="AppKey" value="48293843"/>
    <!-- True：输出调用步骤；False：不输出调用步骤-->
    <add key="Debug" value="True"/>
    <!-- True：服务端输出调用步骤；True：写记录。False：不写记录 -->
    <add key="DebugSsoLog" value="True"/>
    <!-- 设置 图片路径-->
    <add key="uploadUrl" value="/CustomerImage/company"/>
  </appSettings>
  <connectionStrings>
    <add name="ConnectionString2" connectionString="Data Source=.;Initial Catalog=bole_Data;User ID=sa;pwd=Aa123;" providerName="System.Data.SqlClient"/>
    <add name="CnStringMeta2" connectionString="Data Source=.;Initial Catalog=bole_Data;User ID=sa;pwd=Aa123;" providerName="System.Data.SqlClient"/>
    <add name="CnStringRole2" connectionString="Data Source=.;Initial Catalog=bole_Data;User ID=sa;pwd=Aa123;" providerName="System.Data.SqlClient"/>
    <add name="CnStringUser2" connectionString="Data Source=.;Initial Catalog=bole_Data;User ID=sa;pwd=Aa123;" providerName="System.Data.SqlClient"/>
    <add name="ConnectionString" connectionString="server=localhost;database=MoreFish;uid=sa;pwd=admin;charset=utf8mb4;" providerName="MySql.Data.MySqlClient"/>
    <add name="CnStringMeta" connectionString="server=localhost;database=MoreFish;uid=sa;pwd=admin;charset=utf8mb4;" providerName="MySql.Data.MySqlClient"/>
    <add name="CnStringRole" connectionString="server=localhost;database=MoreFish;uid=sa;pwd=admin;charset=utf8mb4;" providerName="MySql.Data.MySqlClient"/>
    <add name="CnStringUser" connectionString="server=localhost;database=MoreFish;uid=sa;pwd=admin;charset=utf8mb4;" providerName="MySql.Data.MySqlClient"/>
  </connectionStrings>
  <system.web>
    <pages validateRequest="false"/>
    <compilation debug="true" tempDirectory="d:\root"/>
    <!--
      通过 <authentication> 节，可配置 
      ASP.NET 用于识别进入用户的 
      安全身份验证模式。
    -->
    <authentication mode="Windows"/>
    <!--
       通过 <customErrors> 节，可以配置
       在执行请求的过程中出现未处理的错误时要执行 
       的操作。具体而言，
       开发人员通过该节可配置要显示的 html 错误页，
       以代替错误堆栈跟踪。

       <customErrors mode="RemoteOnly" defaultRedirect="GenericErrorPage.htm">
         <error statusCode="403" redirect="NoAccess.htm" />
         <error statusCode="404" redirect="FileNotFound.htm" />
       </customErrors>
    -->
  </system.web>
</configuration>